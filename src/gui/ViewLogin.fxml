<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="244.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewLoginController">
   <children>
      <Pane prefHeight="244.0" prefWidth="275.0">
         <children>
            <Label layoutX="48.0" layoutY="33.0" text="Game Memories Login">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="78.0" text="Name User :" />
            <Label layoutX="14.0" layoutY="142.0" text="Password User :" />
            <TextField fx:id="txtNameUser" layoutX="14.0" layoutY="97.0" />
            <TextField fx:id="txtPasswordUser" layoutX="14.0" layoutY="159.0" />
            <Button fx:id="btLogin" layoutX="47.0" layoutY="199.0" mnemonicParsing="false" onAction="#onBtLogin" text="Login" />
            <Button fx:id="btSign" layoutX="163.0" layoutY="199.0" mnemonicParsing="false" onAction="#onBtSign" text="Sign in" />
         </children>
      </Pane>
   </children>
</AnchorPane>
